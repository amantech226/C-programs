#include <stdio.h>
int table(int num,int times);

int main()
{
    int num,times=1;
    
    printf("Enter the number:- ");
    scanf("%d",num);
    
    table(num,times);
    
    return 0;
}

int table(int num,int times)
{
 
    if(times==11)
    {
        return 1;
    }
    
    else
    {
        printf("%d\n",num*times);
        return table(num,times+1);
      
    }
    
}
