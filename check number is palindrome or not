#include <stdio.h>

int main()
{
    int num,rev=0,temp=0;
    
    printf("Enter the number :- ");
    scanf("%d",&num);
    
    temp=num;
    
    while(temp!=0)
    {
        rev=rev*10+temp%10;
        temp/=10;
    }
    
    if(rev==num)
    {
        printf("number is a palindrome");
    }
    
    else
    {
        printf("number is not a palindrome");
    }
    
    
   

    return 0;
}
